---
---
{
  "posts" : 
  {
	"url": [{% for post in site.posts %}
		"{{ post.url }}" {% unless forloop.last %},{% endunless %}
	{% endfor %}],
	
	"heading":[{% for post in site.posts %}
		"{{ post.postHeading }}" {% unless forloop.last %},{% endunless %}
	{% endfor %}],
	
	"date": [{% for post in site.posts %}
		"{{post.date | date_to_string}}" {% unless forloop.last %},{% endunless %}
	{% endfor %}]
  }
}
